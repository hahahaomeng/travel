<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.devMode" value="false" />
	<constant name="struts.action.extension" value="json" />
	<package name="struts" namespace="/" extends="struts-default">
		<action name="user_*" class="com.order.action.UserAction" method="{1}">
		</action>
		<action name="product_*" class="com.order.action.ProductAction" method="{1}">
		</action>
		<action name="order_*" class="com.order.action.OrderAction" method="{1}">
		</action>
		<action name="application_*" class="com.order.action.ApplicationAction" method="{1}">
		</action>
		<action name="comment_*" class="com.order.action.CommentAction" method="{1}">
		</action>
	</package>
</struts>